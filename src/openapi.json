    "/api/solicitud": {
      "post": {
        "tags": [
          "solicitudes"
        ],
        "summary": "Alta de solicitud.",
        "description": "Alta de una nueva solicitud de validación y firma.",
        "operationId": "operationId_8",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "required": [
                  "nrosolicitud",
                  "workflowid",
                  "workflowlabel",
                  "adobemessage",
                  "archivos",
                  "firmantes"
                ],
                "properties": {
                  "nrosolicitud": {
                    "type": "string",
                    "example": "ID_SOLICITUD_VT"
                  },
                  "workflowid": {
                    "type": "string",
                    "example": "xnjklop-31"
                  },
                  "workflowlabel": {
                    "type": "string",
                    "example": "Label 1"
                  },
                  "adobemessage": {
                    "type": "string",
                    "example": "messageInfo - defaultValue"
                  },
                  "archivos": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "format": "binary"
                    }
                  },
                  "firmantes": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "role": {
                          "type": "string"
                        },
                        "label": {
                          "type": "string"
                        },
                        "authenticationmethod": {
                          "type": "string"
                        },
                        "sexo": {
                          "type": "string"
                        },
                        "dni": {
                          "type": "string"
                        },
                        "email": {
                          "type": "string"
                        },
                        "phone": {
                          "type": "string"
                        },
                        "validarenaper": {
                          "type": "boolean"
                        }
                      }
                    },
                    "example": [
                      {
                        "role": "SIGNER",
                        "label": "Asegurado",
                        "authenticationmethod": "NONE",
                        "sexo": "M",
                        "dni": 27310264,
                        "email": "juanmanuel.arancibia@smg.com.ar",
                        "phone": 1133000352,
                        "validarenaper": false
                      }
                    ]
                  }
                }
              }
            }
          },
          "description": "Datos de la nueva solicitud."
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "ok": {
                      "type": "boolean",
                      "example": true
                    },
                    "msg": {
                      "type": "string",
                      "example": ""
                    },
                    "data": {
                      "type": "object",
                      "properties": {
                        "insertedId": {
                          "type": "integer",
                          "example": 1
                        },
                        "rowsAffected": {
                          "type": "integer",
                          "example": 1
                        }
                      }
                    },
                    "errores": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      },
                      "example": []
                    }
                  }
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/requestBodies/Unauthorized"
          },
          "409": {
            "description": "Conflict",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "ok": {
                      "type": "boolean",
                      "example": false
                    },
                    "msg": {
                      "type": "string",
                      "example": "Hay errores en los datos suministrados"
                    },
                    "data": {
                      "type": "string",
                      "example": null
                    },
                    "errores": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      },
                      "example": [
                        "firmantes no es un array válido."
                      ]
                    }
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ]
      }
    },
