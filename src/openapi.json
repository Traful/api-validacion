    "/api/token/validate/{token}": {
      "get": {
        "tags": [
          "login"
        ],
        "summary": "Validación de Token.",
        "description": "Se valida si el token es válido, de estar valido se verifica si aún está vigente o ya expiro.",
        "operationId": "operationId_1",
        "parameters": [
          {
            "in": "path",
            "name": "token",
            "required": true,
            "schema": {
              "type": "string",
              "example": "Bearer eyJhbGciOiJIUzI1NiIsInR5cCI6I..."
            },
            "description": "Bearer Token"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "ok": {
                      "type": "boolean",
                      "example": true
                    },
                    "msg": {
                      "type": "string",
                      "example": ""
                    },
                    "data": {
                      "type": "object",
                      "properties": {
                        "id": {
                          "type": "integer",
                          "example": 1
                        },
                        "email": {
                          "type": "string",
                          "example": "johndoe@server.com"
                        },
                        "firstname": {
                          "type": "string",
                          "example": "John"
                        },
                        "lastname": {
                          "type": "string",
                          "example": "Doe"
                        },
                        "jwt": {
                          "type": "string",
                          "example": "Bearer eyJhbGciOiJIUzI1NiIsInR5cCI6I..."
                        },
                        "errores": {
                          "type": "array",
                          "items": {
                            "type": "string"
                          },
                          "example": []
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/requestBodies/Unauthorized"
          }
        }
      }
    },
